// Place your key bindings in this file to override the defaults
[
  {
    "key": "alt+enter",
    "command": "sqltools.executeQueryFromFile",
    "when": "editorTextFocus && editorLangId == 'sql'",
  },
  {
    "key": "alt+enter",
    "command": "sqltools.executeQuery",
    "when": "editorTextFocus && editorLangId == 'sql' && editorHasSelection == true",
  },
  {
    "key": "alt+enter",
    "command": "sqltools.executeCurrentQuery",
    "when": "editorTextFocus && editorLangId == 'sql'",
  },
  {
    "key": "alt+right",
    "command": "editor.action.revealDeclaration",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+left",
    "command": "workbench.action.navigateBack",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+enter",
    "command": "python.execInTerminal-icon",
    "when": "editorTextFocus && editorLangId == 'python'"
  },
  {
    "key": "ctrl+m ctrl+right",
    "command": "workbench.action.closeEditorsInOtherGroups"
  },
  {
    "key": "ctrl+m ctrl+delete",
    "command": "workbench.action.closeEditorsInGroup"
  },
  {
    "key": "ctrl+m w",
    "command": "-workbench.action.closeEditorsInGroup"
  },
  {
    "key": "ctrl+h",
    "command": "editor.action.showHover",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+alt+h",
    "command": "editor.actions.findWithArgs",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+up",
    "command": "workbench.action.terminal.resizePaneUp",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+down",
    "command": "workbench.action.terminal.resizePaneDown",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+shift+up",
    "command": "workbench.action.terminal.scrollUp",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+shift+down",
    "command": "workbench.action.terminal.scrollDown",
    "when": "terminalFocus"
  },
  {
    "key": "shift+alt+enter",
    "command": "runCommands",
    "args": {
      "commands": [
        "debugpy.debugInTerminal",
        "workbench.debug.action.focusRepl"
      ]
    }
  },
  {
    "key": "ctrl+f5",
    "command": "workbench.debug.action.focusRepl",
    "when": "inDebugMode"
  },
  {
    "key": "f1",
    "command": "workbench.action.debug.continue",
    "when": "inDebugMode"
  },
  {
    "key": "f2",
    "command": "workbench.action.debug.stepOver",
    "when": "inDebugMode"
  },
  {
    "key": "f3",
    "command": "workbench.action.debug.stepInto",
    "when": "inDebugMode"
  },
  {
    "key": "f4",
    "command": "workbench.action.debug.stepOut",
    "when": "inDebugMode"
  },
  {
    "key": "f5",
    "command": "workbench.action.debug.restart",
    "when": "inDebugMode"
  },
  {
    "key": "f6",
    "command": "workbench.action.debug.stop",
    "when": "inDebugMode"
  },
  {
    "key": "ctrl+l",
    "command": "workbench.panel.chat"
  },
  {
    "key": "ctrl+c",
    "command": "workbench.action.chat.cancel",
    "when": "chatInputHasFocus"
  },
  {
    "key": "alt+n",
    "command": "chatEditor.action.undoHunk",
    "when": "chatEdits.cursorInChangeRange && chatEdits.hasEditorModifications && editorFocus && !chatEdits.isCurrentlyBeingModified || chatEdits.cursorInChangeRange && chatEdits.hasEditorModifications && notebookCellListFocused && !chatEdits.isCurrentlyBeingModified"
  },
  {
    "key": "shift+alt+n",
    "command": "chatEditor.action.reject",
    "when": "chatEdits.hasEditorModifications && chatEdits.isGlobalEditingSession && editorFocus && !chatEdits.isCurrentlyBeingModified || chatEdits.hasEditorModifications && chatEdits.isGlobalEditingSession && notebookEditorFocused && !chatEdits.isCurrentlyBeingModified"
  },
  {
    "key": "alt+y",
    "command": "chatEditor.action.acceptHunk",
    "when": "chatEdits.cursorInChangeRange && chatEdits.hasEditorModifications && editorFocus && !chatEdits.isCurrentlyBeingModified || chatEdits.cursorInChangeRange && chatEdits.hasEditorModifications && notebookCellListFocused && !chatEdits.isCurrentlyBeingModified"
  },
  {
    "key": "shift+alt+y",
    "command": "chatEditor.action.acceptAllEdits",
    "when": "chatEdits.hasEditorModifications && editorFocus && !chatEdits.isCurrentlyBeingModified || chatEdits.hasEditorModifications && notebookEditorFocused && !chatEdits.isCurrentlyBeingModified"
  },
  {
    "key": "ctrl+alt+r",
    "command": "git.clean",
    "when": "editorTextFocus && !editorReadonly"
  }
]